{
  "name": "@bakdotdev/dev-tools",
  "version": "1.1.15",
  "license": "MIT",
  "description": "Click-to-source development tool for React, Vue, and Angular. Inspect elements, open source files in your editor, or copy LLM-friendly snippets.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bakdotdev/dev-tools.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "default": "./dist/react.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js",
      "default": "./dist/vue.js"
    },
    "./angular": {
      "types": "./dist/angular.d.ts",
      "import": "./dist/angular.js",
      "default": "./dist/angular.js"
    },
    "./vanilla": {
      "types": "./dist/vanilla.d.ts",
      "import": "./dist/vanilla.js",
      "default": "./dist/vanilla.js"
    },
    "./babel-plugin": {
      "types": "./dist/babel-jsx/index.d.ts",
      "import": "./dist/babel-jsx/index.js"
    },
    "./webpack-loader": {
      "types": "./dist/webpack-loader.d.ts",
      "import": "./dist/webpack-loader.js",
      "require": "./dist/webpack-loader.cjs"
    },
    "./stub": {
      "types": "./dist/stub.d.ts",
      "import": "./dist/stub.js",
      "default": "./dist/stub.js"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.js",
      "default": "./dist/vite-plugin.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "tsc --noEmit --watch",
    "prebuild": "rm -rf dist",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/parser": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0"
  },
  "devDependencies": {
    "@babel/traverse": "^7.24.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@vue/compiler-core": "^3.5.0",
    "@vue/compiler-sfc": "^3.5.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "vue": ">=3.0.0",
    "@angular/core": ">=17.0.0",
    "vite": ">=5.0.0",
    "@vue/compiler-sfc": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "@vue/compiler-sfc": {
      "optional": true
    }
  }
}
